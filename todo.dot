digraph G {
    label="rusnet webdev notes"
    node[
        shape="record"            \
        resolution="100"          \
        fontsize="10"             \
        color="#0AA0DD"           \
        fontname="Free Mono Bold" \
        fontcolor="#0AA0DD"       \
    ]
    bgcolor="#f0f0f0"

    edge[
        color="#0AA0DD"           \
        arrowsize="0.7"           \
        fontsize="6"              \
        shape="insulator"         \
        fontcolor="#0AA0DD"       \
        fontname="Free Mono Bold" \
    ]

    dpi=200
    splines=true

    Item [
        label="{ Item              |\
            - vars & relations - \l|\
            :uid                 \r|\
            :name                \r|\
            :tags_uid            \r|\
            - methods -          \l|\
            :save                \r|\
            :set                 \r|\
            - storage -          \l|\
            :ItemStorage          \r\
        }"
    ]

    RootListForItems [
        label="{ Root List for Items |\
        }"
    ]

    List [
        label="{ List              |\
            - vars & relations - \l|\
            :uid                 \r|\
            :name                \r|\
            :count               \r|\
            :items_uid           \r|\
            - methods -          \l|\
            :set                 \r|\
            :get                  \r\
        }"
    ]

    RoorListForLists [
        label="{ Root List for Lists |\
        }"
    ]

    Tag [
        label="{ Tag               |\
            - vars & relations - \l|\
            :uid                 \r|\
            :name                \r|\
            :items_uid           \r|\
            - methods -          \l|\
                                  \r\
        }"
    ]

    Storage [
        label="{ Storage  |\
            - methods -   |\
            :set\l        |\
            :get\l         \
        }"
    ]

    Functions [
        label="{ Functions    |\
            - gen_uid -     \l|\
            desc            \r|\
            - render me -   \l|\
            desc            \r|\
        }"
    ]

    Process [
        label="{ Process                                 |\
            load data items                            \l|\
            init items                                 \l|\
            load data lists                            \l|\
            init lists                                 \l|\
            load json data\l [if data time is expired] \r|\
            init json data\l [if data time is expired]  \r\
        }"
    ]

    Item -> Storage
    List -> Storage
    Tag -> Storage
}
