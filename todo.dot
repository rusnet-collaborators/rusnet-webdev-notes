digraph G {
    label="rusnet webdev notes"
    node[
        shape="record"            \
        resolution="100"          \
        fontsize="10"             \
        color="#0AA0DD"           \
        fontname="Free Mono Bold" \
        fontcolor="#0AA0DD"       \
    ]
    bgcolor="#f0f0f0"

    edge[
        color="#0AA0DD"           \
        arrowsize="0.7"           \
        fontsize="6"              \
        shape="insulator"         \
        fontcolor="#0AA0DD"       \
        fontname="Free Mono Bold" \
    ]

    dpi=200
    splines=true

    Item [
        label="{ Item              |\
            :uid                 \r|\
            :name                \r|\
            :url                 \r|\
            :description         \r|\
            :progress_of_learn   \r|\
            :color_of_accent     \r|\
            :status              \r|\
                                 \r|\
            :tags_uid            \r|\
            :timer_uid           \r\
        }"
    ]

    Item -> Storage
    Item -> ItemMethods
    Item -> PanelsItem
    Item -> ContentItem

    ItemMethods [
        label="{ methods }"
    ]
    ItemMethods -> { save set }

    TopPanelButtonsList [
        label="{ Top Panel Buttons List     |\
            :config_toggle                \l|\
            :timer_toggle                 \l|\
            :marked_as_remove_toggle      \l\
        }"
    ]

    PanelsItem [ label="{ panels    |\
            :panel_top            \l|\
            :panel_bottom         \l\
        }"
    ]

    PanelsItem -> panel_top
    PanelsItem -> panel_bottom

    ContentItem -> { itemDescription itemUrl }

    panel_top -> buttons -> { config_toggle timer_toggle marked_as_remove_toggle }

    panel_bottom -> area
    area -> { progress_of_learn color_of_accent }

    ListOfMarks [
        label="{ List of Marks     |\
            :progress_of_learn   \l|\
            :color_of_accent     \l\
        }"
    ]

    RootListForItems [
        label="{ Root List for Items |\
        }"
    ]

    List [
        label="{ List              |\
            - vars & relations - \l|\
            :uid                 \r|\
            :name                \r|\
            :count               \r|\
            :items_uid           \r|\
            - methods -          \l|\
            :set                 \r|\
            :get                 \r\
        }"
    ]

    RoorListForLists [
        label="{ Root List for Lists |\
        }"
    ]

    Tag [
        label="{ Tag               |\
            - vars & relations - \l|\
            :uid                 \r|\
            :name                \r|\
            :items_uid           \r|\
            - methods -          \l|\
                                 \r\
        }"
    ]

    Storage [
        label="{ Storage   |\
            - methods -    |\
            :set         \l|\
            :get         \l\
        }"
    ]

    Functions [
        label="{ Functions    |\
            - gen_uid -     \l|\
            desc            \r|\
            - render me -   \l|\
            desc            \r\
        }"
    ]

    Process [
        label="{ Process                                 |\
            load data items                            \l|\
            init items                                 \l|\
            load data lists                            \l|\
            init lists                                 \l|\
            load json data\l [if data time is expired] \r|\
            init json data\l [if data time is expired] \r\
        }"
    ]

    List -> Storage
    Tag -> Storage
}
